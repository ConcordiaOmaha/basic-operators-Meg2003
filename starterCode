// Example program
#include <iostream>
#include <string>

int main()
{

  int num1 = 0; 
  int num2 = 0;
  std::cout << "enter a number"; 
  std::cin >> num1; 
  std::cout<< "enter a second number";
  std::cin >> num2;
  
  int output = num2 + num1;
  std::cout << " Adding your numbers gives " << output;
    
    output = num2 - num1;
  std::cout << " Subtracting your numbers gives " << output;
  
   output = num2 / num1;
  std::cout << " Dividing your numbers gives " << output;
  
  output = num2 * num1;
  std::cout << " Multipying your numbers gives " << output;
  
  output = num2 % num1;
  std::cout << " Modulus of your numbers gives " << output;

}
