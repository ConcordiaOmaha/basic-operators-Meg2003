// Example program
#include <iostream>
#include <string>

int main()
{

  double num1 = 0; 
  double num2 = 0;
  std::cout << "enter a number"; 
  std::cin >> num1; 
  std::cout<< "enter a second number";
  std::cin >> num2;
  
  double output = num2 - num1;
  
  std::cout << " subtracting your numbers gives " << output;
    
}
